public class nestedif {

	public static void main(String[] args) {
		int a=20,b=24,c=29;
		if(a>b) {
			if(a>c) {
				System.out.println("The largest number is: a="+a);
			}
			else {
				System.out.println("The largest number is: c= "+c);
			}
			
		}else {
			if(b>c) {
				System.out.println("The largest number is: b="+b);
			}
			else {
				System.out.println("The largest number is: c= "+c);
			}
			
		}
		

	}

}
